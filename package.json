{
  "name": "webpack-html5-starter",
  "version": "1.0.0",
  "main": "./src/js/app.js",
  "author": "justlstn",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production del-cli dist && webpack --optimize-minimize --mode production --config ./webpack/prod.config.js && html dist/*.html --indent-size 2 --progress --colors",
    "watch": "cross-env NODE_ENV=production webpack --mode development --config ./webpack/dev.config.js --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --config ./webpack/dev.config.js --open --progress --color",
    "dev": "cross-env NODE_ENV=development webpack --mode development --config ./webpack/dev.config.js",
    "clear": "del-cli dist"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "del-cli": "^1.1.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-cli": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^2.0.0",
    "resolve-url-loader": "^3.0.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "wowjs": "^1.1.3"
  }
}
